  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3"
  },
  "dependencies": {
    "bddx-core": "*",
    "conf": "^10.2.0",
    "graphql-zeus": "^5.2.3",
    "inquirer": "^9.1.4",
    "jira.js": "^2.15.14",
    "node-fetch": "^3.3.0",
    "ws": "^8.11.0",
    "yargs": "^17.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aexol-studio/bddx.git"
  },
  "bugs": {
    "url": "https://github.com/aexol-studio/bddx/issues"
  }
